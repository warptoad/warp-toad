  "scripts": {
    "cp-deployments:esm": "cp -r ignition/deployments/ dist/esm/ignition/; cp -r scripts/deploy/aztec/aztecDeployments/ dist/esm/scripts/deploy/aztec/;",
    "cp-deployments:cjs": "cp -r ignition/deployments/ dist/cjs/ignition/; cp -r scripts/deploy/aztec/aztecDeployments/ dist/cjs/scripts/deploy/aztec/;",
    "cp-deployments:browser": "cp -r ignition/deployments/ dist/browser/ignition/; cp -r scripts/deploy/aztec/aztecDeployments/ dist/browser/scripts/deploy/aztec/;",
    "cp-deployments": "yarn cp-deployments:esm && yarn cp-deployments:cjs && yarn cp-deployments:browser",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:browser": "tsc -p tsconfig.browser.json",
    "build": "yarn build:esm && yarn build:cjs && yarn build:browser"
